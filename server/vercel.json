{
    "version" : 2,
    "name" : "Full Stack",
    "builds" : [
        {
            "src" : "index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/"
        }
    ],
    "rewrites": [
        {   
            "source": "/socket.io/:path*", 
            "destination": "/api/socket" 
        }
    ]
}